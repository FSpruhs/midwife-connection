extend type Query {
    getMidwifes: [Midwife]!
    getMidwife(id: ID!): Midwife
}

extend type Mutation {
    createMidwife(
        firstName: String!,
        lastName: String!,
        areas: [Int]!
    ): Midwife!
}

type Midwife {
    id: ID!
    firstName: String!
    lastName: String!
    areas: [Int]!
}